{% if data is not null %}
    {% if data is is_image %}
        <img src="{{ path('kmj_upload_download', {id: data })}}" {{ attributes|raw }}/>
    {% else %}
        {{ download_link(data, options) }}
    {% endif %}
{% else %}
    <img src="{{ asset('bundles/upload/images/no-image.png')}}" {{ attributes|raw }}/>
{% endif %}